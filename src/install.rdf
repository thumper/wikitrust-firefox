<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>wikitrust@soe.ucsc.edu</em:id>
    <em:version>0.4.12</em:version>
    <em:type>2</em:type>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>3.7a3pre</em:maxVersion>
      </Description>
    </em:targetApplication>

   
    <!-- Front End MetaData -->
    <em:name>WikiTrust</em:name>
    <em:description>Add a "trust tab" to Wikipedia, for easy access to the UC Santa Cruz WikiTrust data.</em:description>
    <em:iconURL>chrome://wikitrust/content/wt-logo.png</em:iconURL>
    <em:creator>Bo Adler, Luca de Alfaro, Ian Pye</em:creator>
    <em:homepageURL>http://www.wikitrust.net</em:homepageURL>
  </Description>      
</RDF>
